;;; RevealFunctionPass

(define (square x) (((@function imul 2) x) x))

(define (main)
  ((@function println-non-void 1)
   ((@function square 1) ((@function square 1) 3))))

;;; LiftLambdaPass

(define (square x) (((@function imul 2) x) x))

(define (main)
  ((@function println-non-void 1)
   ((@function square 1) ((@function square 1) 3))))

;;; UnnestOperandPass

(define (square x)
  (@let1 _₁ (@function imul 2) (@let1 _₂ (_₁ x) (_₂ x))))

(define (main)
  (@let1 _₁ (@function println-non-void 1)
    (@let1 _₂ (@function square 1)
      (@let1 _₃ (@function square 1)
        (@let1 _₄ 3
          (@let1 _₅ (_₃ _₄) (@let1 _₆ (_₂ _₅) (_₁ _₆))))))))

;;; ExplicateControlPass

(define-function square
  (block entry
    (= x (argument 0))
    (= _₁ (const (@function imul 2)))
    (= _₂ (apply _₁ x))
    (= _↩ (apply _₂ x))
    (return _↩)))

(define-function main
  (block entry
    (= _₁ (const (@function println-non-void 1)))
    (= _₂ (const (@function square 1)))
    (= _₃ (const (@function square 1)))
    (= _₄ (const 3))
    (= _₅ (apply _₃ _₄))
    (= _₆ (apply _₂ _₅))
    (= _↩ (apply _₁ _₆))
    (return _↩)))

;;; SelectInstructionPass

(define-code square
  (block entry
    (movq (reg rdi) (var x))
    (leaq (deref-label imul) (var _₁))
    (movq (var _↩) (reg rax))
    (retq)))

(define-code main
  (block entry
    (leaq (deref-label println-non-void) (var _₁))
    (leaq (deref-label square) (var _₂))
    (leaq (deref-label square) (var _₃))
    (movq (imm 3) (var _₄))
    (movq (var _↩) (reg rax))
    (retq)))

