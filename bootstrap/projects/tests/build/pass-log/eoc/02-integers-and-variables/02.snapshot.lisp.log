;;; Input

(define (main)
  (println-non-void
   (begin
     (= y (begin (= x 20) (iadd x (begin (= x 22) x))))
     y)))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 y (@let1 x 20 ((iadd x) (@let1 x 22 x))) y)))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 y₁ (@let1 x₁ 20 ((iadd x₁) (@let1 x₂ 22 x₂))) y₁)))

;;; RevealFunctionPass

(define (main)
  ((@function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; LiftLambdaPass

(define (main)
  ((@function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@function println-non-void 1)
    (@let1 y₁
        (@let1 x₁ 20
          (@let1 _₂ (@function iadd 2)
            (@let1 _₃ (_₂ x₁) (@let1 x₂ 22 (_₃ x₂)))))
      (_₁ y₁))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@function println-non-void 1)))
    (= x₁ (const 20))
    (= _₂ (const (@function iadd 2)))
    (= _₃ (apply _₂ x₁))
    (= x₂ (const 22))
    (= y₁ (apply _₃ x₂))
    (= _↩ (apply _₁ y₁))
    (return _↩)))

