;;; Input

(define (main)
  (println-non-void
   (begin
     (= x (random-dice))
     (= y (random-dice))
     (iadd (iadd x y) 42))))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 x (random-dice)
     (@let1 y (random-dice) ((iadd ((iadd x) y)) 42)))))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 x₁ (random-dice)
     (@let1 y₁ (random-dice) ((iadd ((iadd x₁) y₁)) 42)))))

;;; RevealFunctionPass

(define (main)
  ((@function println-non-void 1)
   (@let1 x₁ ((@function random-dice 0))
     (@let1 y₁ ((@function random-dice 0))
       (((@function iadd 2) (((@function iadd 2) x₁) y₁)) 42)))))

;;; LiftLambdaPass

(define (main)
  ((@function println-non-void 1)
   (@let1 x₁ ((@function random-dice 0))
     (@let1 y₁ ((@function random-dice 0))
       (((@function iadd 2) (((@function iadd 2) x₁) y₁)) 42)))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@function println-non-void 1)
    (@let1 x₁ (@let1 _₂ (@function random-dice 0) (_₂))
      (@let1 y₁ (@let1 _₃ (@function random-dice 0) (_₃))
        (@let1 _₄ (@function iadd 2)
          (@let1 _₅ (@function iadd 2)
            (@let1 _₆ (_₅ x₁)
              (@let1 _₇ (_₆ y₁)
                (@let1 _₈ (_₄ _₇)
                  (@let1 _₉ 42 (@let1 _₁₀ (_₈ _₉) (_₁ _₁₀))))))))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@function println-non-void 1)))
    (= _₂ (const (@function random-dice 0)))
    (= x₁ (nullary-apply _₂))
    (= _₃ (const (@function random-dice 0)))
    (= y₁ (nullary-apply _₃))
    (= _₄ (const (@function iadd 2)))
    (= _₅ (const (@function iadd 2)))
    (= _₆ (apply _₅ x₁))
    (= _₇ (apply _₆ y₁))
    (= _₈ (apply _₄ _₇))
    (= _₉ (const 42))
    (= _₁₀ (apply _₈ _₉))
    (= _↩ (apply _₁ _₁₀))
    (return _↩)))

;;; SelectInstructionPass

(define-code main
  (block entry
    (leaq (deref-label println-non-void) (var _₁))
    (leaq (deref-label random-dice) (var _₂))
    (leaq (deref-label random-dice) (var _₃))
    (leaq (deref-label iadd) (var _₄))
    (leaq (deref-label iadd) (var _₅))
    (movq (imm 42) (var _₉))
    (movq (var _↩) (reg rax))
    (retq)))

