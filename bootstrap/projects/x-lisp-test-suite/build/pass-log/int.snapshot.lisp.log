;;; RevealFunctionPass

(define (square x) (((@primitive-function imul 2) x) x))

(define (main)
  ((@primitive-function println-non-void 1)
   ((@function square 1) ((@function square 1) 3))))

;;; LiftLambdaPass

(define (square x) (((@primitive-function imul 2) x) x))

(define (main)
  ((@primitive-function println-non-void 1)
   ((@function square 1) ((@function square 1) 3))))

;;; UnnestOperandPass

(define (square x)
  (@let1 _₁ (@primitive-function imul 2)
    (@let1 _₂ (_₁ x) (_₂ x))))

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 _₂ (@function square 1)
      (@let1 _₃ (@function square 1)
        (@let1 _₄ 3
          (@let1 _₅ (_₃ _₄) (@let1 _₆ (_₂ _₅) (_₁ _₆))))))))

;;; ExplicateControlPass

(define-function square
  (block entry
    (= x (argument 0))
    (= _₁ (const (@primitive-function imul 2)))
    (= _₂ (apply _₁ x))
    (= _↩ (apply _₂ x))
    (return _↩)))

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= _₂ (const (@function square 1)))
    (= _₃ (const (@function square 1)))
    (= _₄ (const 3))
    (= _₅ (apply _₃ _₄))
    (= _₆ (apply _₂ _₅))
    (= _↩ (apply _₁ _₆))
    (return _↩)))

;;; SelectInstructionPass

(define-code square
  (block entry
    (movq (reg rdi) (var x))
    (leaq (deref-label (label imul)) (var _₁))
    (movq (var _₁) (reg rdi))
    (movq (var x) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₂))
    (movq (var _₂) (reg rdi))
    (movq (var x) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

(define-code main
  (block entry
    (leaq (deref-label (label println-non-void)) (var _₁))
    (leaq (deref-label (label square)) (var _₂))
    (leaq (deref-label (label square)) (var _₃))
    (movq (imm 3) (var _₄))
    (movq (var _₃) (reg rdi))
    (movq (var _₄) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₅))
    (movq (var _₂) (reg rdi))
    (movq (var _₅) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₆))
    (movq (var _₁) (reg rdi))
    (movq (var _₆) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
