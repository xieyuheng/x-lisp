                (@let1 _₈ (_₇ z₁)
                  (@let1 _₉ (@primitive-function ineg 1)
                    (@let1 _₁₀ (_₉ y₁)
                      (@let1 _₁₁ (_₈ _₁₀) (_₁ _₁₁)))))))))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= v₁ (const 1))
    (= w₁ (const 42))
    (= _₂ (const (@primitive-function iadd 2)))
    (= _₃ (apply _₂ v₁))
    (= _₄ (const 7))
    (= x₁ (apply _₃ _₄))
    (= y₁ (call identity x₁))
    (= _₅ (const (@primitive-function iadd 2)))
    (= _₆ (apply _₅ x₁))
    (= z₁ (apply _₆ w₁))
    (= _₇ (const (@primitive-function iadd 2)))
    (= _₈ (apply _₇ z₁))
    (= _₉ (const (@primitive-function ineg 1)))
    (= _₁₀ (apply _₉ y₁))
    (= _₁₁ (apply _₈ _₁₀))
    (= _↩ (apply _₁ _₁₁))
    (return _↩)))

