;;; Input

(define (main)
  (println-non-void
   (begin
     (= y 6)
     (begin (= x (begin (= y (ineg 42)) y)) (iadd x y)))))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 y 6 (@let1 x (@let1 y (ineg 42) y) ((iadd x) y)))))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 y₁ 6
     (@let1 x₁ (@let1 y₂ (ineg 42) y₂) ((iadd x₁) y₁)))))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁ 6
     (@let1 x₁
         (@let1 y₂ ((@primitive-function ineg 1) 42) y₂)
       (((@primitive-function iadd 2) x₁) y₁)))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁ 6
     (@let1 x₁
         (@let1 y₂ ((@primitive-function ineg 1) 42) y₂)
       (((@primitive-function iadd 2) x₁) y₁)))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 y₁ 6
      (@let1 x₁
          (@let1 y₂
              (@let1 _₂ (@primitive-function ineg 1)
                (@let1 _₃ 42 (_₂ _₃)))
            y₂)
        (@let1 _₄ (@primitive-function iadd 2)
          (@let1 _₅ (_₄ x₁) (@let1 _₆ (_₅ y₁) (_₁ _₆))))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= y₁ (const 6))
    (= _₂ (const (@primitive-function ineg 1)))
    (= _₃ (const 42))
    (= y₂ (apply _₂ _₃))
    (= x₁ (call identity y₂))
    (= _₄ (const (@primitive-function iadd 2)))
    (= _₅ (apply _₄ x₁))
    (= _₆ (apply _₅ y₁))
    (= _↩ (apply _₁ _₆))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label x-println-non-void)) (var _₁))
    (movq (imm 6) (var y₁))
    (leaq (deref-label (label x-ineg)) (var _₂))
    (movq (imm 42) (var _₃))
    (movq (var _₂) (reg rdi))
    (movq (var _₃) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var y₂))
    (movq (var y₂) (reg rdi))
    (callq-n (label identity) (arity 1))
    (movq (reg rax) (var x₁))
    (leaq (deref-label (label x-iadd)) (var _₄))
    (movq (var _₄) (reg rdi))
    (movq (var x₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₅))
    (movq (var _₅) (reg rdi))
    (movq (var y₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₆))
    (movq (var _₁) (reg rdi))
    (movq (var _₆) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

