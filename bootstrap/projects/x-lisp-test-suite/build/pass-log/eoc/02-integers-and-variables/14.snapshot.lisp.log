;;; Input

(define (main)
  (println-non-void
   (begin (= x 32) (iadd (begin (= x 10) x) x))))

;;; ShrinkPass

(define (main)
  (println-non-void (@let1 x 32 ((iadd (@let1 x 10 x)) x))))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 x₁ 32 ((iadd (@let1 x₂ 10 x₂)) x₁))))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁ 32
     (((@primitive-function iadd 2) (@let1 x₂ 10 x₂)) x₁))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁ 32
     (((@primitive-function iadd 2) (@let1 x₂ 10 x₂)) x₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 x₁ 32
      (@let1 _₂ (@primitive-function iadd 2)
        (@let1 x₂ 10
          (@let1 _₃ (_₂ x₂) (@let1 _₄ (_₃ x₁) (_₁ _₄))))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= x₁ (const 32))
    (= _₂ (const (@primitive-function iadd 2)))
    (= x₂ (const 10))
    (= _₃ (apply _₂ x₂))
    (= _₄ (apply _₃ x₁))
    (= _↩ (apply _₁ _₄))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label x-println-non-void)) (var _₁))
    (movq (imm 32) (var x₁))
    (leaq (deref-label (label x-iadd)) (var _₂))
    (movq (imm 10) (var x₂))
    (movq (var _₂) (reg rdi))
    (movq (var x₂) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₃))
    (movq (var _₃) (reg rdi))
    (movq (var x₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₄))
    (movq (var _₁) (reg rdi))
    (movq (var _₄) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

