   (begin (= x (begin (= x 4) (iadd x 1))) (iadd x 2))))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 x (@let1 x 4 ((iadd x) 1)) ((iadd x) 2))))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 x₁ (@let1 x₂ 4 ((iadd x₂) 1)) ((iadd x₁) 2))))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁
       (@let1 x₂ 4 (((@primitive-function iadd 2) x₂) 1))
     (((@primitive-function iadd 2) x₁) 2))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁
       (@let1 x₂ 4 (((@primitive-function iadd 2) x₂) 1))
     (((@primitive-function iadd 2) x₁) 2))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 x₁
        (@let1 x₂ 4
          (@let1 _₂ (@primitive-function iadd 2)
            (@let1 _₃ (_₂ x₂) (@let1 _₄ 1 (_₃ _₄)))))
      (@let1 _₅ (@primitive-function iadd 2)
        (@let1 _₆ (_₅ x₁)
          (@let1 _₇ 2 (@let1 _₈ (_₆ _₇) (_₁ _₈))))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= x₂ (const 4))
    (= _₂ (const (@primitive-function iadd 2)))
    (= _₃ (apply _₂ x₂))
    (= _₄ (const 1))
    (= x₁ (apply _₃ _₄))
    (= _₅ (const (@primitive-function iadd 2)))
    (= _₆ (apply _₅ x₁))
    (= _₇ (const 2))
    (= _₈ (apply _₆ _₇))
    (= _↩ (apply _₁ _₈))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label println-non-void)) (var _₁))
    (movq (imm 4) (var x₂))
    (leaq (deref-label (label iadd)) (var _₂))
    (movq (var _₂) (reg rdi))
    (movq (var x₂) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₃))
    (movq (imm 1) (var _₄))
    (movq (var _₃) (reg rdi))
    (movq (var _₄) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var x₁))
    (leaq (deref-label (label iadd)) (var _₅))
    (movq (var _₅) (reg rdi))
    (movq (var x₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₆))
    (movq (imm 2) (var _₇))
    (movq (var _₆) (reg rdi))
    (movq (var _₇) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₈))
    (movq (var _₁) (reg rdi))
    (movq (var _₈) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

