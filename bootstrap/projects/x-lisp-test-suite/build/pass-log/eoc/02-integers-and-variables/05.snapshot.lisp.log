;;; Input

(define (main)
  (println-non-void (begin (= a 42) (= b a) b)))

;;; ShrinkPass

(define (main)
  (println-non-void (@let1 a 42 (@let1 b a b))))

;;; UniquifyPass

(define (main)
  (println-non-void (@let1 a₁ 42 (@let1 b₁ a₁ b₁))))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 a₁ 42 (@let1 b₁ a₁ b₁))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 a₁ 42 (@let1 b₁ a₁ b₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 a₁ 42 (@let1 b₁ a₁ (_₁ b₁)))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= a₁ (const 42))
    (= b₁ (call identity a₁))
    (= _↩ (apply _₁ b₁))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label x-println-non-void)) (var _₁))
    (movq (imm 42) (var a₁))
    (movq (var a₁) (reg rdi))
    (callq-n (label identity) (arity 1))
    (movq (reg rax) (var b₁))
    (movq (var _₁) (reg rdi))
    (movq (var b₁) (reg rsi))
    (callq-n (label x-apply-unary) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

