;;; Input

(define (main)
  (println-non-void
   (begin
     (= y (begin (= x 20) (iadd x (begin (= x 22) x))))
     y)))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 y (@let1 x 20 ((iadd x) (@let1 x 22 x))) y)))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 y₁ (@let1 x₁ 20 ((iadd x₁) (@let1 x₂ 22 x₂))) y₁)))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@primitive-function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@primitive-function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 y₁
        (@let1 x₁ 20
          (@let1 _₂ (@primitive-function iadd 2)
            (@let1 _₃ (_₂ x₁) (@let1 x₂ 22 (_₃ x₂)))))
      (_₁ y₁))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= x₁ (const 20))
    (= _₂ (const (@primitive-function iadd 2)))
    (= _₃ (apply _₂ x₁))
    (= x₂ (const 22))
    (= y₁ (apply _₃ x₂))
    (= _↩ (apply _₁ y₁))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (label-deref (label x-println-non-void)) (var _₁))
    (movq (imm 20) (var x₁))
    (leaq (label-deref (label x-iadd)) (var _₂))
    (movq (var _₂) (reg rdi))
    (movq (var x₁) (reg rsi))
    (callq-n (label x-apply-unary) (arity 2))
    (movq (reg rax) (var _₃))
    (movq (imm 22) (var x₂))
    (movq (var _₃) (reg rdi))
    (movq (var x₂) (reg rsi))
    (callq-n (label x-apply-unary) (arity 2))
    (movq (reg rax) (var y₁))
    (movq (var _₁) (reg rdi))
    (movq (var y₁) (reg rsi))
    (callq-n (label x-apply-unary) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

