;;; Input

(define (main)
  (println-non-void
   (begin
     (= y (begin (= x 20) (begin (= z 22) (iadd x z))))
     y)))

;;; ShrinkPass

(define (main)
  (println-non-void
   (@let1 y (@let1 x 20 (@let1 z 22 ((iadd x) z))) y)))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 y₁ (@let1 x₁ 20 (@let1 z₁ 22 ((iadd x₁) z₁))) y₁)))

;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (@let1 z₁ 22
           (((@primitive-function iadd 2) x₁) z₁)))
     y₁)))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (@let1 z₁ 22
           (((@primitive-function iadd 2) x₁) z₁)))
     y₁)))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 y₁
        (@let1 x₁ 20
          (@let1 z₁ 22
            (@let1 _₂ (@primitive-function iadd 2)
              (@let1 _₃ (_₂ x₁) (_₃ z₁)))))
      (_₁ y₁))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= x₁ (const 20))
    (= z₁ (const 22))
    (= _₂ (const (@primitive-function iadd 2)))
    (= _₃ (apply _₂ x₁))
    (= y₁ (apply _₃ z₁))
    (= _↩ (apply _₁ y₁))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label println-non-void)) (var _₁))
    (movq (imm 20) (var x₁))
    (movq (imm 22) (var z₁))
    (leaq (deref-label (label iadd)) (var _₂))
    (movq (var _₂) (reg rdi))
    (movq (var x₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₃))
    (movq (var _₃) (reg rdi))
    (movq (var z₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var y₁))
    (movq (var _₁) (reg rdi))
    (movq (var y₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

