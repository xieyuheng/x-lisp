;;; RevealFunctionPass

(define (main)
  ((@primitive-function println-non-void 1)
   (@let1 z₁
       (@let1 y₁
           (@let1 x₁ ((@primitive-function ineg 1) 42) x₁)
         y₁)
     ((@primitive-function ineg 1) z₁))))

;;; LiftLambdaPass

(define (main)
