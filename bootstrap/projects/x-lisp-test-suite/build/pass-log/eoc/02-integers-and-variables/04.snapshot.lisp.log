   (@let1 z (@let1 y (@let1 x (ineg 42) x) y) (ineg z))))

;;; UniquifyPass

(define (main)
  (println-non-void
   (@let1 z₁ (@let1 y₁ (@let1 x₁ (ineg 42) x₁) y₁)
