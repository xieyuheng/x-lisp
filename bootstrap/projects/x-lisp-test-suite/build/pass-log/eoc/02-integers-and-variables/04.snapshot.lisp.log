   (@let1 z₁
       (@let1 y₁
           (@let1 x₁ ((@primitive-function ineg 1) 42) x₁)
         y₁)
     ((@primitive-function ineg 1) z₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 z₁
        (@let1 y₁
            (@let1 x₁
                (@let1 _₂ (@primitive-function ineg 1)
                  (@let1 _₃ 42 (_₂ _₃)))
              x₁)
          y₁)
      (@let1 _₄ (@primitive-function ineg 1)
        (@let1 _₅ (_₄ z₁) (_₁ _₅))))))

;;; ExplicateControlPass

(define-function main
  (block entry
    (= _₁ (const (@primitive-function println-non-void 1)))
    (= _₂ (const (@primitive-function ineg 1)))
    (= _₃ (const 42))
    (= x₁ (apply _₂ _₃))
    (= y₁ (call identity x₁))
    (= z₁ (call identity y₁))
    (= _₄ (const (@primitive-function ineg 1)))
    (= _₅ (apply _₄ z₁))
    (= _↩ (apply _₁ _₅))
    (return _↩)))

;;; SelectInstructionPass

(export)

(define-code main
  (block entry
    (leaq (deref-label (label x-println-non-void)) (var _₁))
    (leaq (deref-label (label x-ineg)) (var _₂))
    (movq (imm 42) (var _₃))
    (movq (var _₂) (reg rdi))
    (movq (var _₃) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var x₁))
    (movq (var x₁) (reg rdi))
    (callq-n (label identity) (arity 1))
    (movq (reg rax) (var y₁))
    (movq (var y₁) (reg rdi))
    (callq-n (label identity) (arity 1))
    (movq (reg rax) (var z₁))
    (leaq (deref-label (label x-ineg)) (var _₄))
    (movq (var _₄) (reg rdi))
    (movq (var z₁) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₅))
    (movq (var _₁) (reg rdi))
    (movq (var _₅) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
