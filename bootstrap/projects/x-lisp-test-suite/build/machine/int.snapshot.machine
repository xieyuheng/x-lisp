(export)

(define-code square
  (block entry
    (movq (reg rdi) (var x))
    (leaq (deref-label (label x-imul)) (var _₁))
    (movq (var _₁) (reg rdi))
    (movq (var x) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₂))
    (movq (var _₂) (reg rdi))
    (movq (var x) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

(define-code main
  (block entry
    (leaq (deref-label (label x-println-non-void)) (var _₁))
    (leaq (deref-label (label square)) (var _₂))
    (leaq (deref-label (label square)) (var _₃))
    (movq (imm 3) (var _₄))
    (movq (var _₃) (reg rdi))
    (movq (var _₄) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₅))
    (movq (var _₂) (reg rdi))
    (movq (var _₅) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _₆))
    (movq (var _₁) (reg rdi))
    (movq (var _₆) (reg rsi))
    (callq-n (label apply) (arity 2))
    (movq (reg rax) (var _↩))
    (movq (var _↩) (reg rax))
    (retq)))

