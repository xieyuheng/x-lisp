(export _start)

(define-data message
  (chunk entry
    (string "Hello, World!\n"))
  (chunk length
    (dq 14)))

(define-code _start
  (block entry
    (movq (imm 1) (reg rax))
    (movq (imm 1) (reg rdi))
    (leaq (deref-label (label message)) (reg rsi))
    (movq (deref-label (label message.length)) (reg rdx))
    (syscall)
    (movq (imm 60) (reg rax))
    (movq (imm 0) (reg rdi))
    (syscall)))
