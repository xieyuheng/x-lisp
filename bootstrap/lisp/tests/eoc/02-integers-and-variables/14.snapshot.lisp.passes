   (begin (= x 32) (iadd (begin (= x 10) x) x))))

;;; ShrinkPass

(define (_∑-top-level-computation)
  (println-non-void (@let1 x 32 (iadd (@let1 x 10 x) x))))

;;; UniquifyPass

(define (_∑-top-level-computation)
  (println-non-void (@let1 x₁ 32 (iadd (@let1 x₂ 10 x₂) x₁))))

;;; RevealFunctionPass

(define (_∑-top-level-computation)
  ((@function println-non-void 1)
   (@let1 x₁ 32 ((@function iadd 2) (@let1 x₂ 10 x₂) x₁))))

;;; LiftLambdaPass

(define (_∑-top-level-computation)
