(define-function main
  (block entry
    (goto print-bool))

  (block print-bool
    (= _ (const #t))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (const #f))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (goto print-int))

  (block print-int
    (= _ (const 0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (const 1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (const -1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (goto print-float))

  (block print-float
    (= _ (const 0.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (const 1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (const -1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (return)))
