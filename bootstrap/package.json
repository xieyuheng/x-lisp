{
  "name": "@xieyuheng/x-lisp-boot",
  "version": "0.10.3",
  "repository": "github:xieyuheng/x-lisp-boot",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "files": [
    "src",
    "lib",
    "lisp",
    "bin"
  ],
  "bin": {
    "x-lisp-boot": "lib/main.js"
  },
  "scripts": {
    "build": "cp ../runtime/bin/runtime.o bin && tsc",
    "build:watch": "cp ../runtime/bin/runtime.o bin && tsc --watch",
    "test": "node --test 'src/**/*.test.ts'",
    "test-suite:bun": "bun src/main.ts project:test --config ../test-suite/project.json",
    "test-suite:node": "node --stack-size=65536 src/main.ts project:test --config ../test-suite/project.json",
    "test-suite:deno": "deno --allow-all --v8-flags='--stack-size=65536' src/main.ts project:test --config ../test-suite/project.json",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@xieyuheng/command.js": "^0.3.1",
    "@xieyuheng/x-sexp.js": "^0.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3"
  }
}
