{
  "name": "@xieyuheng/x-lisp-boot",
  "version": "0.8.0",
  "repository": "github:xieyuheng/x-lisp-boot",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "files": [
    "src",
    "lib",
    "lisp"
  ],
  "bin": {
    "x-lisp-boot": "lib/main.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "node --test 'src/**/*.test.ts'",
    "test-suite:node": "node --stack-size=65536 src/main.ts project:test --config projects/x-lisp-test-suite/project.json",
    "test-suite:bun": "bun src/main.ts project:test --config projects/x-lisp-test-suite/project.json",
    "test-suite:deno": "deno --allow-all --v8-flags='--stack-size=65536' src/main.ts project:test --config projects/x-lisp-test-suite/project.json",
    "format": "prettier src --write"
  },
  "dependencies": {
    "@xieyuheng/command.js": "^0.2.3",
    "@xieyuheng/x-sexp.js": "^0.5.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3"
  }
}
