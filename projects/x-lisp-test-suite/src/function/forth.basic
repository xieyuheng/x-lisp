(define-function identity
  (block body (= x (argument 0)) (return x)))

(define-function swap
  (block body
    (= f (argument 0))
    (= x (argument 1))
    (= y (argument 2))
    (= _₁ (apply f y))
    (= _↩ (apply _₁ x))
    (return _↩)))

(define-variable _swap/constant)

(define-setup _swap/setup
  (block body
    (= function-address (literal (@address swap)))
    (= arity (literal 3))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        function-address
        arity
        size))
    (store _swap/constant curry)
    (return)))

(define-function drop
