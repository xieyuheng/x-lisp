(define-function square
  (block body
    (= x (argument 0))
    (= _₁ (literal (@primitive-function imul 2)))
    (= _₂ (apply _₁ x))
    (= _↩ (apply _₂ x))
    (return _↩)))

(define-variable square©constant)

(define-setup square©setup
  (block body
    (= address (literal (@address square)))
    (= arity (literal 1))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        address
        arity
        size))
    (store square©constant curry)
    (return)))

(define-function _main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= _₂ (literal (@function square 1)))
    (= _₃ (literal 3))
    (= _₄ (apply _₂ _₃))
    (= _↩ (apply _₁ _₄))
    (return _↩)))

(define-variable _main©constant)

(define-setup _main©setup
  (block body
    (= address (literal (@address _main)))
    (= arity (literal 0))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        address
        arity
        size))
    (store _main©constant curry)
    (return)))
