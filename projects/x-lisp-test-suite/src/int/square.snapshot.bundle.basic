(define-function _main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= _₂ (literal (@function §₁/square 1)))
    (= _₃ (literal (@function §₁/square 1)))
    (= _₄ (literal 3))
    (= _₅ (apply _₃ _₄))
    (= _₆ (apply _₂ _₅))
    (= _↩ (apply _₁ _₆))
    (return _↩)))

(define-variable __main/constant)

(define-setup __main/setup
  (block body
    (= function-address (literal (@address _main)))
    (= arity (literal 0))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        function-address
        arity
        size))
    (store __main/constant curry)
    (return)))

(define-function §₁/square
  (block body
    (= x (argument 0))
    (= _₁ (literal (@primitive-function imul 2)))
    (= _₂ (apply _₁ x))
    (= _↩ (apply _₂ x))
    (return _↩)))

(define-variable §₁/_square/constant)

(define-setup §₁/_square/setup
  (block body
    (= function-address (literal (@address §₁/square)))
    (= arity (literal 1))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        function-address
        arity
        size))
    (store §₁/_square/constant curry)
    (return)))
