(define-function ©two/function
  (block body
    (= _₁ (const (@primitive-function iadd 2)))
    (= _₂ (const 1))
    (= _₃ (apply _₁ _₂))
    (= _₄ (call (@function ©one/get 0)))
    (= _↩ (apply _₃ _₄))
    (return _↩)))

