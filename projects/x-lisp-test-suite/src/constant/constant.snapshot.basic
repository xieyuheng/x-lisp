(define-variable one)

(define-variable ©one/flag #f)

(define-function ©one/get
  (block body
    (= flag (load ©one/flag))
    (branch flag cached init))
  (block cached (= result (load one)) (return result))
  (block init
    (= result (call (@function ©one/function 0)))
    (store one result)
    (= true (const #t))
    (store ©one/flag true)
    (return result)))

(define-function ©one/function
  (block body (= _↩ (const 1)) (return _↩)))

