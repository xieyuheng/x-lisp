(export main)

(define-function main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= _₃ (literal #t))
    (branch _₃ main/then₂ main/else₃))
  (block main/let-body₁ (= _↩ (apply _₁ _₂)) (return _↩))
  (block main/then₂
    (= _₂ (literal 1))
    (goto main/let-body₁))
  (block main/else₃
    (= _₂ (literal 2))
    (goto main/let-body₁)))
