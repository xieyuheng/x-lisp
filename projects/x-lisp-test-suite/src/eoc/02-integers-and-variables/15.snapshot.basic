(define-function _main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= x₂ (literal 4))
    (= _₂ (literal (@primitive-function iadd 2)))
    (= _₃ (apply _₂ x₂))
    (= _₄ (literal 1))
    (= x₁ (apply _₃ _₄))
    (= _₅ (literal (@primitive-function iadd 2)))
    (= _₆ (apply _₅ x₁))
    (= _₇ (literal 2))
    (= _₈ (apply _₆ _₇))
    (= _↩ (apply _₁ _₈))
