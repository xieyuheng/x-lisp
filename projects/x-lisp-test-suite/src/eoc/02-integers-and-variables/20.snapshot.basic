(define-function _main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= x₁ (literal 1))
    (= _₂ (literal (@primitive-function iadd 2)))
    (= _₃ (apply _₂ x₁))
    (= y₁ (literal 5))
    (= _₄ (literal (@primitive-function iadd 2)))
    (= _₅ (apply _₄ y₁))
    (= x₂ (apply _₅ x₁))
    (= _₆ (literal (@primitive-function iadd 2)))
    (= _₇ (apply _₆ x₂))
    (= _₈ (literal 100))
    (= _₉ (apply _₇ _₈))
    (= _₁₀ (apply _₃ _₉))
