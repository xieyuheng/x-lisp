(define-function _main
  (block body
    (= _₁ (literal (@primitive-function println-non-void 1)))
    (= x₁ (literal 4))
    (= _₂ (literal (@primitive-function iadd 2)))
    (= _₃ (literal 8))
    (= _₄ (apply _₂ _₃))
    (= _₅ (apply _₄ x₁))
    (= _↩ (apply _₁ _₅))
    (return _↩)))

(define-variable _main©constant)

(define-setup _main©setup
  (block body
    (= address (literal (@address _main)))
    (= arity (literal 0))
    (= size (literal 0))
    (= curry
       (call
        (@primitive-function make-curry 3)
        address
        arity
        size))
    (store _main©constant curry)
    (return)))
