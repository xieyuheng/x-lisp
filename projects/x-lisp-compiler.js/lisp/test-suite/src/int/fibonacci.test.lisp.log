;;; UnnestOperandPass

(define (fibonacci n)
  (if (int-less-or-equal? n 1)
    n
    (let ((_₁ (isub n 1)))
      (let ((_₂ (fibonacci _₁)))
        (let ((_₃ (isub n 2)))
          (let ((_₄ (fibonacci _₃))) (iadd _₂ _₄)))))))

(define (main)
  (begin
    (let ((_₁ (fibonacci 0))) (assert-equal 0 _₁))
    (begin
      (let ((_₂ (fibonacci 1))) (assert-equal 1 _₂))
      (begin
        (let ((_₃ (fibonacci 2))) (assert-equal 1 _₃))
        (begin
          (let ((_₄ (fibonacci 3))) (assert-equal 2 _₄))
          (begin
            (let ((_₅ (fibonacci 4))) (assert-equal 3 _₅))
            (begin
              (let ((_₆ (fibonacci 5))) (assert-equal 5 _₆))
              (begin
                (let ((_₇ (fibonacci 6)))
                  (assert-equal 8 _₇))
                (begin
                  (let ((_₈ (fibonacci 7)))
                    (assert-equal 13 _₈))
                  (begin
                    (let ((_₉ (fibonacci 8)))
                      (assert-equal 21 _₉))
                    (let ((_₁₀ (fibonacci 9)))
                      (assert-equal 34 _₁₀))))))))))))

