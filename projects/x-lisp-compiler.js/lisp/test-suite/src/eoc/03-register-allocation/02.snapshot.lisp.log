;;; Input

(define (main)
  (= x (random-dice))
  (= y (random-dice))
  (println (iadd (iadd x y) 42)))

;;; ShrinkPass

(define (main)
  (@let1 x (random-dice)
    (@let1 y (random-dice) (println (iadd (iadd x y) 42)))))

;;; UniquifyPass

(define (main)
  (@let1 x₁ (random-dice)
    (@let1 y₁ (random-dice)
      (println (iadd (iadd x₁ y₁) 42)))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁ ((@primitive-function-ref random-dice 0))
    (@let1 y₁ ((@primitive-function-ref random-dice 0))
      ((@primitive-function-ref println 1)
       ((@primitive-function-ref iadd 2)
        ((@primitive-function-ref iadd 2) x₁ y₁)
        42)))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁ ((@primitive-function-ref random-dice 0))
    (@let1 y₁ ((@primitive-function-ref random-dice 0))
      ((@primitive-function-ref println 1)
       ((@primitive-function-ref iadd 2)
        ((@primitive-function-ref iadd 2) x₁ y₁)
        42)))))

;;; UnnestOperandPass

(define (main)
  (@let1 x₁ ((@primitive-function-ref random-dice 0))
    (@let1 y₁ ((@primitive-function-ref random-dice 0))
      (@let1 _₁ ((@primitive-function-ref iadd 2) x₁ y₁)
        (@let1 _₂ ((@primitive-function-ref iadd 2) _₁ 42)
          ((@primitive-function-ref println 1) _₂))))))

