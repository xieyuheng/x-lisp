;;; Input

(define (main)
  (= x (random-dice))
  (= y (random-dice))
  (println (iadd (iadd x y) 42)))

;;; ShrinkPass

(define (main)
  (@let1 x (random-dice)
    (@let1 y (random-dice) (println (iadd (iadd x y) 42)))))

;;; UniquifyPass

(define (main)
  (@let1 x₁ (random-dice)
    (@let1 y₁ (random-dice) (println (iadd (iadd x₁ y₁) 42)))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁ ((@primitive random-dice 0))
    (@let1 y₁ ((@primitive random-dice 0))
      ((@primitive println 1)
       ((@primitive iadd 2) ((@primitive iadd 2) x₁ y₁) 42)))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁ ((@primitive random-dice 0))
    (@let1 y₁ ((@primitive random-dice 0))
      ((@primitive println 1)
       ((@primitive iadd 2) ((@primitive iadd 2) x₁ y₁) 42)))))

;;; UnnestOperandPass

(define (main)
  (@let1 x₁ ((@primitive random-dice 0))
    (@let1 y₁ ((@primitive random-dice 0))
      (@let1 _₁ ((@primitive iadd 2) x₁ y₁)
        (@let1 _₂ ((@primitive iadd 2) _₁ 42)
          ((@primitive println 1) _₂))))))

