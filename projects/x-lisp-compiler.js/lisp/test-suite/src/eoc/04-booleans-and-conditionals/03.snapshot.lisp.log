;;; Input

(define (main)
  (println
   (if
       (and (equal? (random-dice) 1)
            (equal? (random-dice) 2))
     0
     42)))

;;; ShrinkPass

(define (main)
  (println
   (if
       (if ((equal? (random-dice)) 1)
         ((equal? (random-dice)) 2)
         #f)
     0
     42)))

;;; UniquifyPass

(define (main)
  (println
   (if
       (if ((equal? (random-dice)) 1)
         ((equal? (random-dice)) 2)
         #f)
     0
     42)))

;;; RevealGlobalPass

(define (main)
  ((@builtin-function-ref println 1)
   (if
       (if
           (((@builtin-function-ref equal? 2)
             ((@builtin-function-ref random-dice 0)))
            1)
         (((@builtin-function-ref equal? 2)
           ((@builtin-function-ref random-dice 0)))
          2)
         #f)
     0
     42)))

;;; LiftLambdaPass

(define (main)
  ((@builtin-function-ref println 1)
   (if
       (if
           (((@builtin-function-ref equal? 2)
             ((@builtin-function-ref random-dice 0)))
            1)
         (((@builtin-function-ref equal? 2)
           ((@builtin-function-ref random-dice 0)))
          2)
         #f)
     0
     42)))

