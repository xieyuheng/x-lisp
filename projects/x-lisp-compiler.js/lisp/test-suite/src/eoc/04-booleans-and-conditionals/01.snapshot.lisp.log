;;; UnnestOperandPass

(define (main)
  (let ((x₁ (random-dice)))
    (let ((y₁ (random-dice)))
      (let
          ((_₁ (if
                   (if (int-less? x₁ 1)
                     (equal? x₁ 0)
                     (equal? x₁ 2))
                 (iadd y₁ 2)
                 (iadd y₁ 10))))
        (println _₁)))))

