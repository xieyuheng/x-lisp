;;; Input

(define (main)
  (= x (iadd 42 (ineg 10)))
  (println (iadd x 10)))

;;; ShrinkPass

(define (main)
  (@let1 x (iadd 42 (ineg 10)) (println (iadd x 10))))

;;; UniquifyPass

(define (main)
  (@let1 x₁ (iadd 42 (ineg 10)) (println (iadd x₁ 10))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁
      ((@primitive-function-ref iadd 2)
       42
       ((@primitive-function-ref ineg 1) 10))
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref iadd 2) x₁ 10))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁
      ((@primitive-function-ref iadd 2)
       42
       ((@primitive-function-ref ineg 1) 10))
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref iadd 2) x₁ 10))))

