;;; Input

(define (main)
  (= x (iadd 42 (ineg 10)))
  (println (iadd x 10)))

;;; ShrinkPass

(define (main)
  (@let1 x ((iadd 42) (ineg 10)) (println ((iadd x) 10))))

;;; UniquifyPass

(define (main)
  (@let1 x₁ ((iadd 42) (ineg 10)) (println ((iadd x₁) 10))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁
      (((@builtin-function-ref iadd 2) 42)
       ((@builtin-function-ref ineg 1) 10))
    ((@builtin-function-ref println 1)
     (((@builtin-function-ref iadd 2) x₁) 10))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁
      (((@builtin-function-ref iadd 2) 42)
       ((@builtin-function-ref ineg 1) 10))
    ((@builtin-function-ref println 1)
     (((@builtin-function-ref iadd 2) x₁) 10))))

