;;; Input

(define (main) (println (iadd (iadd 1 2) (iadd 3 4))))

;;; ShrinkPass

(define (main) (println (iadd (iadd 1 2) (iadd 3 4))))

;;; UniquifyPass

(define (main) (println (iadd (iadd 1 2) (iadd 3 4))))

;;; RevealGlobalPass

(define (main)
  ((@primitive-function-ref println 1)
   ((@primitive-function-ref iadd 2)
    ((@primitive-function-ref iadd 2) 1 2)
    ((@primitive-function-ref iadd 2) 3 4))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function-ref println 1)
   ((@primitive-function-ref iadd 2)
    ((@primitive-function-ref iadd 2) 1 2)
    ((@primitive-function-ref iadd 2) 3 4))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ ((@primitive-function-ref iadd 2) 1 2)
    (@let1 _₂ ((@primitive-function-ref iadd 2) 3 4)
      (@let1 _₃ ((@primitive-function-ref iadd 2) _₁ _₂)
        ((@primitive-function-ref println 1) _₃)))))

