;;; Input

(define (main) (println (iadd (iadd 1 2) (iadd 3 4))))

;;; ShrinkPass

(define (main) (println ((iadd ((iadd 1) 2)) ((iadd 3) 4))))

;;; UniquifyPass

(define (main) (println ((iadd ((iadd 1) 2)) ((iadd 3) 4))))

;;; RevealGlobalPass

(define (main)
  ((@builtin-function-ref println 1)
   (((@builtin-function-ref iadd 2)
     (((@builtin-function-ref iadd 2) 1) 2))
    (((@builtin-function-ref iadd 2) 3) 4))))

;;; LiftLambdaPass

(define (main)
  ((@builtin-function-ref println 1)
   (((@builtin-function-ref iadd 2)
     (((@builtin-function-ref iadd 2) 1) 2))
    (((@builtin-function-ref iadd 2) 3) 4))))

