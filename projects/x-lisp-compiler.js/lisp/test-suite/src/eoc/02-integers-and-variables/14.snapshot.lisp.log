;;; Input

(define (main) (= x 32) (println (iadd (begin (= x 10) x) x)))

;;; ShrinkPass

(define (main)
  (let ((x 32)) (println (iadd (let ((x 10)) x) x))))

;;; UniquifyPass

(define (main)
  (let ((x₁ 32)) (println (iadd (let ((x₂ 10)) x₂) x₁))))

;;; RevealGlobalPass

(define (main)
  (let ((x₁ 32))
    ((@primitive println 1)
     ((@primitive iadd 2) (let ((x₂ 10)) x₂) x₁))))

;;; LiftLambdaPass

(define (main)
  (let ((x₁ 32))
    ((@primitive println 1)
     ((@primitive iadd 2) (let ((x₂ 10)) x₂) x₁))))

;;; UnnestOperandPass

(define (main)
  (let ((x₁ 32))
    (let ((x₂ 10))
      (let ((_₁ ((@primitive iadd 2) x₂ x₁)))
        ((@primitive println 1) _₁)))))

