;;; Input

(define (main)
  (= x 32)
  (println (iadd (begin (= x 10) x) x)))

;;; ShrinkPass

(define (main)
  (@let1 x 32 (println (iadd (@let1 x 10 x) x))))

;;; UniquifyPass

(define (main)
  (@let1 x₁ 32 (println (iadd (@let1 x₂ 10 x₂) x₁))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁ 32
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref iadd 2) (@let1 x₂ 10 x₂) x₁))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁ 32
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref iadd 2) (@let1 x₂ 10 x₂) x₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 x₁ 32
    (@let1 x₂ 10
      (@let1 _₁ ((@primitive-function-ref iadd 2) x₂ x₁)
        ((@primitive-function-ref println 1) _₁)))))

