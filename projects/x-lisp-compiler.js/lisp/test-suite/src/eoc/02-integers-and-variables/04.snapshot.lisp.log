;;; Input

(define (main)
  (= z (begin (= y (begin (= x (ineg 42)) x)) y))
  (println (ineg z)))

;;; ShrinkPass

(define (main)
  (@let1 z (@let1 y (@let1 x (ineg 42) x) y)
    (println (ineg z))))

;;; UniquifyPass

(define (main)
  (@let1 z₁ (@let1 y₁ (@let1 x₁ (ineg 42) x₁) y₁)
    (println (ineg z₁))))

;;; RevealGlobalPass

(define (main)
  (@let1 z₁
      (@let1 y₁
          (@let1 x₁ ((@primitive-function-ref ineg 1) 42)
            x₁)
        y₁)
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref ineg 1) z₁))))

;;; LiftLambdaPass

(define (main)
  (@let1 z₁
      (@let1 y₁
          (@let1 x₁ ((@primitive-function-ref ineg 1) 42)
            x₁)
        y₁)
    ((@primitive-function-ref println 1)
     ((@primitive-function-ref ineg 1) z₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 z₁
      (@let1 y₁
          (@let1 x₁ ((@primitive-function-ref ineg 1) 42)
            x₁)
        y₁)
    (@let1 _₁ ((@primitive-function-ref ineg 1) z₁)
      ((@primitive-function-ref println 1) _₁))))

