;;; Input

(define (main)
  (= z (begin (= y (begin (= x (ineg 42)) x)) y))
  (println (ineg z)))

;;; ShrinkPass

(define (main)
  (let ((z (let ((y (let ((x (ineg 42))) x))) y)))
    (println (ineg z))))

;;; UniquifyPass

(define (main)
  (let ((z₁ (let ((y₁ (let ((x₁ (ineg 42))) x₁))) y₁)))
    (println (ineg z₁))))

;;; RevealGlobalPass

(define (main)
  (let
      ((z₁ (let ((y₁ (let ((x₁ ((@primitive ineg 1) 42))) x₁)))
             y₁)))
    ((@primitive println 1) ((@primitive ineg 1) z₁))))

;;; LiftLambdaPass

(define (main)
  (let
      ((z₁ (let ((y₁ (let ((x₁ ((@primitive ineg 1) 42))) x₁)))
             y₁)))
    ((@primitive println 1) ((@primitive ineg 1) z₁))))

;;; UnnestOperandPass

(define (main)
  (let
      ((z₁ (let ((y₁ (let ((x₁ ((@primitive ineg 1) 42))) x₁)))
             y₁)))
    (let ((_₁ ((@primitive ineg 1) z₁)))
      ((@primitive println 1) _₁))))

