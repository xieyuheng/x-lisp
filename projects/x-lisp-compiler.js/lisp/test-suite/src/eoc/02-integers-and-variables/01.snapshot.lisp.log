;;; Input

(define (main) (= x 4) (println (iadd 8 x)))

;;; ShrinkPass

(define (main) (@let1 x 4 (println (iadd 8 x))))

;;; UniquifyPass

(define (main) (@let1 x₁ 4 (println (iadd 8 x₁))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁ 4
    ((@primitive-ref println 1)
     ((@primitive-ref iadd 2) 8 x₁))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁ 4
    ((@primitive-ref println 1)
     ((@primitive-ref iadd 2) 8 x₁))))

