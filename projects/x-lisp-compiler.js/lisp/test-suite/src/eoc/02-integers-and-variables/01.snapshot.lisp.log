(define (main) (= x 4) (println (iadd 8 x)))

;;; ShrinkPass

(define (main) (let ((x 4)) (println (iadd 8 x))))

;;; UniquifyPass

(define (main) (let ((x₁ 4)) (println (iadd 8 x₁))))

;;; RevealGlobalPass

(define (main)
  (let ((x₁ 4))
    ((@primitive println 1) ((@primitive iadd 2) 8 x₁))))

;;; LiftLambdaPass

(define (main)
  (let ((x₁ 4))
    ((@primitive println 1) ((@primitive iadd 2) 8 x₁))))

;;; UnnestOperandPass

(define (main)
  (let ((x₁ 4))
    (let ((_₁ ((@primitive iadd 2) 8 x₁)))
      ((@primitive println 1) _₁))))

