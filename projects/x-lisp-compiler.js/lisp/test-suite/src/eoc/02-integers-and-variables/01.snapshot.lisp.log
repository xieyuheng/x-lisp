;;; Input

(define (main) (= x 4) (println (iadd 8 x)))

;;; ShrinkPass

(define (main) (@let1 x 4 (println (iadd 8 x))))

;;; UniquifyPass

(define (main) (@let1 x₁ 4 (println (iadd 8 x₁))))

;;; RevealGlobalPass

(define (main)
  (@let1 x₁ 4
    ((@primitive-function-ref println 1) ((@primitive-function-ref iadd 2) 8 x₁))))

;;; LiftLambdaPass

(define (main)
  (@let1 x₁ 4
    ((@primitive-function-ref println 1) ((@primitive-function-ref iadd 2) 8 x₁))))

;;; UnnestOperandPass

(define (main)
  (@let1 x₁ 4
    (@let1 _₁ ((@primitive-function-ref iadd 2) 8 x₁)
      ((@primitive-function-ref println 1) _₁))))

