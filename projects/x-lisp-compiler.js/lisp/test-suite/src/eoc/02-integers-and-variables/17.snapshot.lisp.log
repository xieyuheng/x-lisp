;;; Input

(define (main) (println (iadd 1 (iadd (random-dice) 1))))

;;; ShrinkPass

(define (main)
  (println ((iadd 1) ((iadd (random-dice)) 1))))

;;; UniquifyPass

(define (main)
  (println ((iadd 1) ((iadd (random-dice)) 1))))

;;; RevealGlobalPass

(define (main)
  ((@builtin-function-ref println 1)
   (((@builtin-function-ref iadd 2) 1)
    (((@builtin-function-ref iadd 2)
      ((@builtin-function-ref random-dice 0)))
     1))))

;;; LiftLambdaPass

(define (main)
  ((@builtin-function-ref println 1)
   (((@builtin-function-ref iadd 2) 1)
    (((@builtin-function-ref iadd 2)
      ((@builtin-function-ref random-dice 0)))
     1))))

