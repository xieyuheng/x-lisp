;;; Input

(define (main) (println (iadd 1 (iadd (random-dice) 1))))

;;; ShrinkPass

(define (main) (println (iadd 1 (iadd (random-dice) 1))))

;;; UniquifyPass

(define (main) (println (iadd 1 (iadd (random-dice) 1))))

;;; RevealGlobalPass

(define (main)
  ((@primitive-function-ref println 1)
   ((@primitive-function-ref iadd 2)
    1
    ((@primitive-function-ref iadd 2)
     ((@primitive-function-ref random-dice 0))
     1))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function-ref println 1)
   ((@primitive-function-ref iadd 2)
    1
    ((@primitive-function-ref iadd 2)
     ((@primitive-function-ref random-dice 0))
     1))))

;;; UnnestOperandPass

(define (main)
  (@let1 _₁ ((@primitive-function-ref random-dice 0))
    (@let1 _₂ ((@primitive-function-ref iadd 2) _₁ 1)
      (@let1 _₃ ((@primitive-function-ref iadd 2) 1 _₂)
        ((@primitive-function-ref println 1) _₃)))))

