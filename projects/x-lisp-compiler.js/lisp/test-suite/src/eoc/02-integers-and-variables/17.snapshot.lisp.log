;;; Input

(define (main) (println (iadd 1 (iadd (random-dice) 1))))

;;; ShrinkPass

(define (main)
  (println ((iadd 1) ((iadd (random-dice)) 1))))

;;; UniquifyPass

(define (main)
  (println ((iadd 1) ((iadd (random-dice)) 1))))

;;; RevealGlobalPass

(define (main)
  ((@primitive-function println 1)
   (((@primitive-function iadd 2) 1)
    (((@primitive-function iadd 2)
      ((@primitive-function random-dice 0)))
     1))))

;;; LiftLambdaPass

(define (main)
  ((@primitive-function println 1)
   (((@primitive-function iadd 2) 1)
    (((@primitive-function iadd 2)
      ((@primitive-function random-dice 0)))
     1))))

