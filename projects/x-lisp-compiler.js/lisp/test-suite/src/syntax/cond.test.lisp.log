;;; Input

(define (main)
  (assert-equal 1 (cond (#t (begin 1)) (#t (begin 2))))
  (assert-equal 2 (cond (#f (begin 1)) (#t (begin 2))))
  (cond
    (#t (begin (assert true)))
    (#t (begin (assert false))))
  (cond
    (#f (begin (assert false)))
    (#t (begin (assert true)))))

