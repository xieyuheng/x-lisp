(define-function (main)
  (block entry (= _₁ 1) (goto let-body₅))
  (block let-body₁ (= _₆ true) (return (assert _₆)))
  (block let-body₂ (goto let-body₁))
  (block let-body₃
    (perform (assert-equal 2 _₂))
    (= _₃ true)
    (perform (assert _₃))
    (goto let-body₁))
  (block let-body₄ (goto let-body₃))
  (block let-body₅
    (perform (assert-equal 1 _₁))
    (= _₂ 2)
    (goto let-body₄))
  (block let-body₆ (goto let-body₅)))
