(define-function (main)
  (block entry (= _₁ 1) (goto (@label let-body₅)))
  (block let-body₁ (= _₆ true) (return (assert _₆)))
  (block let-body₂ (goto (@label let-body₁)))
  (block let-body₃
    (perform (assert-equal 2 _₂))
