(define-function (main)
  (block entry (test (equal? 1 1)) (branch then₁₃ else₁₄))
  (block then₁ (= _₅ false) (return (assert _₅)))
  (block else₂ (= _₆ true) (return (assert _₆)))
  (block let-body₃ (test (equal? 1 2)) (branch then₁ else₂))
  (block let-body₄ (goto let-body₃))
  (block then₅
    (= _₃ true)
    (perform (assert _₃))
    (goto let-body₃))
  (block else₆
    (= _₄ false)
    (perform (assert _₄))
    (goto let-body₄))
  (block let-body₇
    (perform (assert-equal 2 _₂))
    (test (equal? 1 1))
    (branch then₅ else₆))
  (block let-body₈ (goto let-body₇))
  (block then₉ (= _₂ 1) (goto let-body₇))
  (block else₁₀ (= _₂ 2) (goto let-body₈))
  (block let-body₁₁
    (perform (assert-equal 1 _₁))
    (test (equal? 1 2))
    (branch then₉ else₁₀))
  (block let-body₁₂ (goto let-body₁₁))
  (block then₁₃ (= _₁ 1) (goto let-body₁₁))
  (block else₁₄ (= _₁ 2) (goto let-body₁₂)))
