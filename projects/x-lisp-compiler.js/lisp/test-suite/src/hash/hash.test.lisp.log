;;; Input

(define (main) (assert (hash-empty? (@hash))))

;;; ShrinkPass

(define (main)
  (assert (hash-empty? (@let1 hash (make-hash) hash))))

;;; UniquifyPass

(define (main)
  (assert (hash-empty? (@let1 hash₁ (make-hash) hash₁))))

;;; RevealGlobalPass

(define (main)
  (assert
    ((@primitive-function-ref hash-empty? 1)
     (@let1 hash₁ ((@primitive-function-ref make-hash 0))
       hash₁))))

;;; LiftLambdaPass

(define (main)
  (assert
    ((@primitive-function-ref hash-empty? 1)
     (@let1 hash₁ ((@primitive-function-ref make-hash 0))
       hash₁))))

